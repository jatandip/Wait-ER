        android:text=">"
        android:textColor="#546AE4"
        android:textSize="20sp"
        app:fontFamily="sans-serif-condensed-medium"
        app:layout_constraintBottom_toBottomOf="@+id/BTNLethbridge"
        app:layout_constraintEnd_toEndOf="@+id/BTNLethbridge"
        app:layout_constraintTop_toTopOf="@+id/BTNLethbridge" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="43dp"
        android:layout_height="42dp"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:gravity="center_horizontal|center_vertical"
        android:text=">"
        android:textColor="#546AE4"
        android:textSize="20sp"
        app:fontFamily="sans-serif-condensed-medium"
        app:layout_constraintBottom_toBottomOf="@+id/BTNMedicineHat"
        app:layout_constraintEnd_toEndOf="@+id/BTNMedicineHat"
        app:layout_constraintTop_toTopOf="@+id/BTNMedicineHat" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="43dp"
        android:layout_height="42dp"
        android:layout_marginEnd="12dp"
        android:layout_marginRight="12dp"
        android:gravity="center_horizontal|center_vertical"
        android:text=">"
        android:textColor="#546AE4"
        android:textSize="20sp"
        app:fontFamily="sans-serif-condensed-medium"
        app:layout_constraintBottom_toBottomOf="@+id/BTNRedDeer"
        app:layout_constraintEnd_toEndOf="@+id/BTNRedDeer"
        app:layout_constraintTop_toTopOf="@+id/BTNRedDeer" />

</androidx.constraintlayout.widget.ConstraintLayout>