        android:gravity="center_horizontal|center_vertical"
        android:text=">"
        android:textColor="#546AE4"
        android:textSize="20sp"
        app:fontFamily="sans-serif-condensed-medium"
        app:layout_constraintBottom_toBottomOf="@+id/BTNEdmonton"
